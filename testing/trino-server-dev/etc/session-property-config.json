[
    {
        "group": "global.*",
        "sessionProperties": {
            "query_max_execution_time": "8h"
        }
    },
    {
        "group": "global.interactive.*",
        "sessionProperties": {
            "query_max_execution_time": "1h"
        }
    },
    {
        "group": "global.pipeline.*",
        "clientTags": ["etl"],
        "sessionProperties": {
            "scale_writers": "true",
            "writer_min_size": "1GB",
            "hive.insert_existing_partitions_behavior": "overwrite"
        }
    }
]
