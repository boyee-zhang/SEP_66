{
    "rules": [
        {
            "pattern": "(.+?)@example.com",
            "user": "$1_file"
        },
        {
            "pattern": "([^@]+)(@.*)?",
            "user": "$1$2",
            "case": "lower"
        },
        {
            "pattern": "test",
            "allow": false
        },
        {
            "pattern": "(.*)"
        }
    ]
}
